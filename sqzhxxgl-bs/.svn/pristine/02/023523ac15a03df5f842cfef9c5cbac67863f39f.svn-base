<%@ page language="java" pageEncoding="UTF-8"%>
  <%@taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>


  <link href="/zui/footable/css/footable.core.css" rel="stylesheet" type="text/css"/>

	 <link href="/zui/footable/css/arbitrary_toggle_markup.css" rel="stylesheet" type="text/css"/>
      <script src="/zui/footable/js/footable.js" type="text/javascript"></script>
      <script src="/zui/footable/js/footable.js" type="text/javascript"></script>
<script>
		
		$(function() {
		 $('table').footable({
                            toggleHTMLElement: '<span><img style="width:14px;height: 14px"  src="<c:url value="/styles/images/plus.png"/>" class="footable-toggle footable-expand" border="0" alt="Expand">'
                            + '<img style="width:14px;height: 14px" src="<c:url value="/styles/images/minus.png"/>" class="footable-toggle footable-contract" border="0" alt="Contract"></span>'
                       });
		});
		     
	</script>